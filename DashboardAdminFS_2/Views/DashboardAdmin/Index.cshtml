        <h1>Objetos de carrousel</h1>
    </div>
    <div class="col-lg-6 col-md-6" col-md-6">

        <div align="center">

            <div class="card" style="padding:18px">
                <div class="card-header"> <h3>Agregar item al slider</h3></div>
                <div class="card-body">

                    <form method="post" enctype="multipart/form-data" action="/DashboardAdmin/InsertItemCarrousel">
                        <input type="text" name="id_userCreated" class="hidden" value="@User.Identity.GetUserId()" />

                        <img id="PreviewImage_itemCarousel" class="" width="180" height="240" src=""
                             alt="Imagen de crédito" style="height:100px;" />

                        <br>
                        <input name="files" id="file_prodfin" onchange="readURL_itemCarousel(this);" type="file" required />
                        <script>
                            function readURL_itemCarousel(input) {
                                if (input.files && input.files[0]) {//Check if input has files.
                                    var reader = new FileReader(); //Initialize FileReader.
                                    reader.onload = function (e) {
                                        $('#PreviewImage_itemCarousel').attr('src', e.target.result);
                                        $("#PreviewImage_itemCarousel").resizable({ aspectRatio: true, maxWidth: 300 });
                                    };
                                    reader.readAsDataURL(input.files[0]);
                                }
                                else {
                                    $('#PreviewImage_itemCarousel').attr('src', "#");
                                }
                            }
                        </script>                <label>Cabecera de slider</label><br />
                        <input type="text" name="h2tag" class="form-control"
                               placeholder="Cabecera Sumate Descuentos EJEMPLO" /><br />
                        <label>complemento de slider</label><br />
                        <input type="text" name="ptag" class="form-control"
                               placeholder="Nuestros descuentos son de EJEMPLO" />
                        <br />
                        <input type="submit" class="form-control btn btn-default" name="" id=""
                               value="Agregar Item de carrousel" style="font-size:10pt" />
                    </form>
                </div>
            </div>


        </div>

    </div>
    <div class="col-lg-6 col-md-6">

        <div class="card" style="padding:18px">
