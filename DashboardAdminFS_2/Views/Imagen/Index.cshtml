<div class="row">
    <div class="col-lg-12" id="carrouselDiv">
        <h1>Objetos de carrousel</h1>
    </div>
    <div class="col-lg-6 col-md-6" col-md-6">

        <div align="center">

            <div class="card" style="padding:18px">
                <div class="card-header"> <h3>Agregar item al slider</h3></div>
                <div class="card-body">

                    <form method="post" enctype="multipart/form-data" action="/DashboardAdmin/InsertItemCarrousel">
                        <input type="text" name="id_userCreated" class="hidden" value="@User.Identity.GetUserId()" />

                        <img id="PreviewImage_itemCarousel" class="" width="180" height="240" src=""
                             alt="Imagen de crédito" style="height:100px;" />

                        <br>
                        <input name="files" id="file_prodfin" onchange="readURL_itemCarousel(this);" type="file" required />
                        <script>
                            function readURL_itemCarousel(input) {
                                if (input.files && input.files[0]) {//Check if input has files.
                                    var reader = new FileReader(); //Initialize FileReader.
                                    reader.onload = function (e) {
                                        $('#PreviewImage_itemCarousel').attr('src', e.target.result);
                                        $("#PreviewImage_itemCarousel").resizable({ aspectRatio: true, maxWidth: 300 });
                                    };
                                    reader.readAsDataURL(input.files[0]);
                                }
                                else {
                                    $('#PreviewImage_itemCarousel').attr('src', "#");
                                }
                            }
                        </script>                <label>Cabecera de slider</label><br />
                        <input type="text" name="h2tag" class="form-control"
                               placeholder="Cabecera Sumate Descuentos EJEMPLO" /><br />
                        <label>complemento de slider</label><br />
                        <input type="text" name="ptag" class="form-control"
                               placeholder="Nuestros descuentos son de EJEMPLO" />
                        <br />
                        <input type="submit" class="form-control btn btn-default" name="" id=""
                               value="Agregar Item de carrousel" />
                    </form>
                </div>
            </div>


        </div>

    </div>
    <div class="col-lg-6 col-md-6">

        <div class="card" style="padding:18px">
            <div class="card-header"><h3>Carrousel <a href="/DashboardAdmin/carrousel/" style="font-size:10pt"  class="btn btn-default pull-right">Ver carrousel</a></h3></div>
            <div class="card-body">

                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h4>
                            Tabla de item en el carrousel
                        </h4>
                    </div>
                    <table class="table table-fixed">
                        <thead>
                            <tr>
                                <th class="col-xs-4">#</th>
                                <th class="col-xs-4"></th>
                                <th class="col-xs-4"></th>
                            </tr>
                        </thead>
                        <tbody id="Carrousel-t"></tbody>
                    </table>
                </div>

                <script>
                        $.ajax({
                            url: "/DashboardAdmin/CarrouselItemsListJson",
                            type: "GET",
                            dataType: "json",
                            //contentType: "application/json; charset=utf-8",

                            success: function (response) {


                                console.log(response)
                                //alert(response);


                                var resplength = response.length;


                                for (let i = 0; i <= resplength; i++) {
                                    document.getElementById('Carrousel-t').innerHTML += `
                                             <tr>
                                                <td class="col-xs-4"> <img src="`+ response[i].img + `" height="30px" alt="Alternate Text" /></td>
                                                <td class="col-xs-4"><font size="3">`+ response[i].h2tag + ` </font></td>
                                                <td class="col-xs-4"><a href="" style="font-size:10pt"  onclick="" class="btn btn-default">Ver</a>
                                                <a style="font-size:10pt"  href="#" onclick="showDeleteCarrouselItem(`+ response[i].id + `)" class="btn btn-danger">Eliminar</a></td>
                                            </tr>
                                            `;
                                }


                            },

                            error: function (jqXHR, textStatus, err) {
                                //$('#myModalLabel').append(`error: - ${err}`);
                                console.log(jqXHR);
                                console.log(textStatus);
                                console.log(err);

                                alert(err + 'err');

                            }

                        });


                        function showDeleteCarrouselItem(id) {
                            var idx = '@User.Identity.GetUserId()';

                            document.getElementById('modal-header').innerHTML = 'Eliminar Carrousel Item';
                            document.getElementById('modal-body').innerHTML = `
                        <div align="center">
                         <h4>¿Estás seguro que deseas eliminar este Carrousel Item?</h4>
                        <form action="/DashboardAdmin/DeleteCarrousellItem" method="post">
                            <input type="text" class="hidden" name="id_userCreated" value="`+ idx + `" />
                            <input type="text" class="hidden" name="id" value="`+ id + `" />
                            <input type="submit" class="btn btn-danger" name="name" value="Si estoy seguro, eliminar" />

                        </form>

                        </div>
                `;


                            $("#exampleModal").modal("show");
                        }




                </script>
            </div>
        </div>



    </div>
</div>


